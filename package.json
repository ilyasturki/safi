{
    "name": "pure-editor",
    "type": "module",
    "private": true,
    "scripts": {
        "prepare": "nuxt prepare",
        "dev": "nuxt dev",
        "build": "nuxt build",
        "preview": "nuxt preview",
        "start": "node  --env-file=.env .output/server/index.mjs",
        "typecheck": "nuxt prepare && vue-tsc -b --noEmit",
        "format": "prettier --write '**/*.{js,ts,vue,css,scss,html,json,md,yml}'",
        "format:check": "prettier --check '**/*.{js,ts,vue,css,scss,html,json,md,yml}'",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "clear": "rm -rf .nuxt/ .output/ node_modules/ dist/"
    },
    "dependencies": {
        "@fontsource/ibm-plex-mono": "^5.2.7",
        "@tailwindcss/vite": "^4.1.15",
        "nuxt": "^4.1.3",
        "tailwindcss": "^4.1.15",
        "vue": "^3.5.22",
        "vue-router": "^4.6.3"
    },
    "devDependencies": {
        "@ilyasso/eslint-config": "^0.0.3",
        "eslint": "^9.38.0",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "typescript": "^5.9.3",
        "vue-tsc": "^3.1.1"
    }
}
